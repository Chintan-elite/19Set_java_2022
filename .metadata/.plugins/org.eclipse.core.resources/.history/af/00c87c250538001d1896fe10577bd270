package oops;

class Student
{
	int id;
	String name;
	
	Student(int id, String name)
	{
		id =id;
		name=name;
	}
	
	public void display()
	{
		System.out.println(id+" "+name);
	}
}
public class O003_Constructordemo {

	
	public static void main(String[] args) {
		
		Student s1 = new Student(10,"tops");
		s1.display();
		
		Student s2 = new Student(20,"tech");
		s2.display();
		
	}
	
}
