package oops;

import com.sun.jdi.Method;

class A
{
	int a = 10;
	
	public void display()
	{
		System.out.println("running display...");
	}
}

class B
{
	public void run()
	{
		System.out.println("Running run Method.class..");
	}
}

public class O004_InheritanceDemo {
	public static void main(String[] args) {
		
		
		B b = new B();
		b.run();
		
	}
}
